<?xml version="1.0" encoding="utf-8" ?> 

<report template="EstimationList.xlsx">
<sheet name="見積実績一覧">
  <cells>
    <cell row="3" col="C">$user.JpName</cell>
    <cell row="4" col="C">$createDate</cell>
  </cells>
  
  <list name="estimations" begin_row="7">
    <cell col="B">$sheet.estimationCd</cell>
    <cell>$sheet.issueDate</cell>
    <cell>$sheet.customerName</cell>
    <cell>$sheet.modelCd</cell>
    <cell>$sheet.categoryType</cell>
    <cell>$sheet.amount</cell>
    <cell>$sheet.estimationOkDays</cell>
    <cell>$sheet.payment</cell>
    <cell>$sheet.status</cell>
  </list>
</sheet>
</report>
